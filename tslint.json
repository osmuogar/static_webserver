{
  "defaultSeverity": "warning",
  "rules": {
    "comment-format": [
      true,
      "check-space",
      "check-uppercase"
    ],
    "eofline": true,
    "file-header": true,
    "import-spacing": true,
    "indent": [
      true,
      "spaces",
      2
    ],
    "jsdoc-format": true,
    "linebreak-style": [
      true,
      "LF"
    ],
    "max-line-length": [
      true,
      80
    ],
    "no-consecutive-blank-lines": true,
    "no-duplicate-imports": true,
    "no-irregular-whitespace": true,
    "no-require-imports": true,
    "ordered-imports": true,
    "semicolon": true,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  }
}